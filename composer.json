{
    "name": "vanssata/vanssata-pag-online-sdk",
    "type": "library",
    "description": "PagOnlineSDK Imprese PHP SDK for developers",
    "keywords": ["Bulbank Italia", "VanssataPagOnlineSDK Imprese", "PHP SDK"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Kakarov",
            "email": "ivan.kakurov@gmail.com"
        }
    ],
    "version": "v0.0.2",
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "guzzlehttp/guzzle": "~6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16|^3.0",
        "phpunit/phpunit": "^7.5|^8.0|^9.0",
        "phpstan/phpstan": "^1.4",
        "orchestra/testbench": "^3.7|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "VanssataPagOnlineSDK\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VanssataPagOnlineSDK\\Tests\\": "tests/"
        }
    }
}
